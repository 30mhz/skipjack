{
    "name": "mytable",
    "schema": {
        "hashkey": {
            "name": "email_address",
            "type": "STRING"
        },
        "rangekey": {
            "name": "check_id",
            "type": "STRING"
        }
    },
    "throughput": {
        "read": 2,
        "write": 4
    },
    "indexes": [
        {
            "name": "enabled_index",
	        "attribute": {
	            "name": "enabled",
	            "type": "STRING"
            }
        },
        {
            "name": "frequency_index",
	        "attribute": {
	            "name": "frequency",
	            "type": "NUMBER"
            }
        },
        {
            "name": "state_index",
	        "attribute": {
	            "name": "state",
	            "type": "STRING"
            }
        }
    ],
    "transformations": [
        {
            "name": "locations",
            "type": "STRING_SET"
        },
        {
            "name": "period",
            "type": "OBSOLETE"
        },
        {
            "name": "max_recent_downs",
            "type": "OBSOLETE"
        }
    ]
}
